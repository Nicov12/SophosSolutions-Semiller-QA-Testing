{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "default": [],
    "title": "Root Schema",
    "items": {
        "type": "object",
        "default": {},
        "title": "A Schema",
        "required": [
            "id",
            "name",
            "data"
        ],
        "properties": {
            "id": {
                "type": "string",
                "default": "",
                "title": "The id Schema",
                "examples": [
                    "3"
                ]
            },
            "name": {
                "type": "string",
                "default": "",
                "title": "The name Schema",
                "examples": [
                    "Apple iPhone 12 Pro Max"
                ]
            },
            "data": {
                "type": "object",
                "default": {},
                "title": "The data Schema",
                "required": [
                    "color",
                    "capacity GB"
                ],
                "properties": {
                    "color": {
                        "type": "string",
                        "default": "",
                        "title": "The color Schema",
                        "examples": [
                            "Cloudy White"
                        ]
                    },
                    "capacity GB": {
                        "type": "integer",
                        "default": 0,
                        "title": "The capacity GB Schema",
                        "examples": [
                            512
                        ]
                    }
                },
                "examples": [{
                    "color": "Cloudy White",
                    "capacity GB": 512
                }]
            }
        },
        "examples": [{
            "id": "3",
            "name": "Apple iPhone 12 Pro Max",
            "data": {
                "color": "Cloudy White",
                "capacity GB": 512
            }
        }]
    },
    "examples": [
        [{
            "id": "3",
            "name": "Apple iPhone 12 Pro Max",
            "data": {
                "color": "Cloudy White",
                "capacity GB": 512
            }
        }]
    ]
}